{
    "version": 2,
    "buildCommand": "sh vercel.sh",
    "builds": [
      {
        "src": "alpine/wsgi.py",
        "use": "@vercel/python",
        "config": { 
            "runtime": "python3.12",
            "buildCommand": "poetry install --no-dev && poetry run setup_prod"

        }
      },
      {
        "src": "static/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "alpine/wsgi.py"
      }
    ]
  }
  