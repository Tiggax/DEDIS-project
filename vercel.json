{
    "version": 2,
    "outputDirectory": ".",
    "builds": [
        {
            "src": "alpine/wsgi.py",
            "use": "@black-raven/vercel-poetry"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "alpine/wsgi.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "static/$1"
        }
    ]
  }
  