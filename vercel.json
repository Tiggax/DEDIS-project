{
    "version": 2,
    "buildCommand": "python3 -m poetry run setup_prod",
    "outputDirectory": "static",
    "installCommand": "pip3 install pysqlite3 setuptools poetry && python3 -m poetry install --no-dev",
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/$1"
        },
        {
            "src": "/(.*)",
            "dest": "api/wsgi.py"
        }
    ]
  }
  
