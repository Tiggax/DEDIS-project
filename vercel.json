{
    "version": 2,
    "outputDirectory": ".",
    "routes": [
        {
            "src": "/(.*)",
            "dest": "alpine/wsgi.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "static/$1"
        }
    ],
    "functions": {
        "alpine/wsgi.py": {
            "runtime": "0vercel/python@3.12"
        }
    }
  }
  