{
    "version": 2,
    "functions": {
        "alpine/wsgi.py": {
          "runtime": "@vercel/python"
        }
    },
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "alpine/wsgi.py"
      }
    ]
  }
  